{
	"name": "sanatorium/path-installers",
	"type": "composer-plugin",
	"description": "Custom composer path installers",
    "authors": [
        {
            "name": "Sanatorium",
            "email": "info@sanatorium.ninja",
            "homepage": "http://sanatorium.ninja"
        },
        {
            "name": "rozklad",
            "email": "jan.rozklad@gmail.com",
            "homepage": "http://rozklad.me"
        }
    ],
    "keywords": [
        "laravel",
        "cartalyst", 
        "platform",
        "extension",
        "composer",
        "installer"
    ],
	"require": {
		"php": ">=5.3.0",
		"composer-plugin-api": "^1.0",
		"cartalyst/composer-installers": ">=1.2.0"
	},
	"autoload": {
		"psr-4": {
			"Sanatorium\\PathInstallers\\": "src/"
		}
	},
	"extra": {
		"component": "package",
		"class": [
			"Sanatorium\\PathInstallers\\PathInstallerPlugin"
		],
		"branch-alias": {
			"dev-master": "1.2-dev"
		}
	}
}
